{
  "private": true,
  "name": "OpenWorkEngine",
  "version": "0.0.1",
  "scripts": {
    "pkg": "./ci/pkg.sh",
    "schema": "yarn graphql-codegen --watch",
    "icons": "cd src/core && node ./generate-icons.js",
    "start": "NODE_OPTIONS=\"--trace-warnings\" PORT=8001 react-scripts start",
    "package": "yarn run pkg build core",

    "ows-app": "./ci/run-dir.sh \"OpenWorkShop/App\" \"yarn run start\"",
    "ows-server": "./ci/run-dir.sh \"OpenWorkShop\" \"dotnet watch run\"",
    "frontend": "concurrently \"yarn run start\" \"yarn run pkg dev core\" \"yarn run schema\" \"yarn run ows-app\"",
    "backend": "concurrently --raw \"cd Makerverse && dotnet watch run\" \"yarn run ows-server\"",

    "build": "lerna run build",
    "build:changed": "lerna run --since origin/master --include-dependents build",
    "lint": "lerna exec -- eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:changed": "lerna exec --since origin/master --include-dependents -- eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "lerna run test",
    "test:changed": "lerna run --since origin/master --include-dependents test",
    "test:update": "lerna run test -- -u",
    "test:update:changed": "lerna run --since origin/master --include-dependents test -- -u",
    "typecheck": "lerna exec -- tsc --noEmit",
    "typecheck:changed": "lerna exec --since origin/master --include-dependents -- tsc --noEmit"
  },
  "resolutions": {
    "subscriptions-transport-ws": "^0.9.18",
    "nib/stylus": "0.54.8",
    "@material-ui/core": "5.0.0-alpha.23",
    "@material-ui/lab": "5.0.0-alpha.23",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0"
  },
  "workspaces": [
    "./src/core",
    "./apps/**"
  ],
  "dependencies": {
