    "ts-jest": "^26.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*/src/*.{png,jpeg,jpg,gif,svg}": "imagemin-lint-staged",
    "./src/*/src/*.{js,jsx,ts,tsx,md,html,css}": "prettier --write",
    "./src/*/src/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
