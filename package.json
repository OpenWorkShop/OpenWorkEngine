{
  "private": true,
  "name": "OpenWorkEngine",
  "version": "0.0.1",
  "scripts": {
    "pkg": "./ci/pkg.sh",
    "schema": "yarn graphql-codegen --watch",
    "icons": "cd src/core && node ./generate-icons.js",
    "start": "NODE_OPTIONS=\"--trace-warnings\" PORT=8001 react-scripts start",
    "build": "react-scripts build",
    "dev": "concurrently \"yarn run start\" \"yarn run pkg dev core\"",
    "ows": "cd OpenWorkShop/App && yarn run start"
  },
  "resolutions": {
    "subscriptions-transport-ws": "^0.9.18",
    "nib/stylus": "0.54.8",
    "@material-ui/core": "5.0.0-alpha.17",
    "@material-ui/lab": "5.0.0-alpha.17",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0"
  },
  "workspaces": [
    "./src/core/dist",
    "./OpenWorkShop/App",
    "./Makerverse/App"
  ],
  "devDependencies": {
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@craco/craco": "^6.0.0",
    "craco-alias": "^2.1.1",
    "@graphql-codegen/cli": "^1.19.4",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-graphql-request": "^2.0.3",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@graphql-codegen/typescript-resolvers": "^1.18.0",
    "@graphql-codegen/graphql-modules-preset": "^1.1.0",
    "@graphql-codegen/add": "^2.0.2",
    "@trendmicro/babel-config": "^1.0.0-alpha",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.1",
    "@testing-library/user-event": "^12.1.10",
    "@types/colornames": "^1.1.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/pubsub-js": "^1.8.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/semver": "^7.3.4",
    "@types/superagent": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "bundle-loader": "^0.5.6",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "imagemin-lint-staged": "^0.4.0",
    "levenary": "^1.1.1",
    "mini-css-extract-plugin": "^1.3.2",
    "pretty-quick": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*/src/*.{png,jpeg,jpg,gif,svg}": "imagemin-lint-staged",
    "./src/*/src/*.{js,jsx,ts,tsx,md,html,css}": "prettier --write",
    "./src/*/src/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
