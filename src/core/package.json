  "scripts": {
    "repackage": "cp ./package.prod.json ./dist/package.json",
    "clean": "rm -rf node_modules && rm -rf ./dist && mkdir ./dist && rm -rf ./tsconfig.tsbuildinfo && yarn run repackage",
    "uninstall": "rm -rf ./node_modules && rm -rf yarn.lock",
    "reinstall": "yarn run uninstall && yarn",
    "generate": "graphql-codegen --config codegen.yml",
