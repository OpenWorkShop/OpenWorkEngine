  "scripts": {
    "clean": "find . -regex \"./dist/.*\\.[jt]s$\" -type f -delete && find . -regex \"./dist/.*\\.map$\" -type f -delete && find . -regex \"./dist/*\" -type d -delete",
    "uninstall": "rm -rf ./node_modules && rm -rf yarn.lock",
    "reinstall": "yarn run uninstall && yarn",
    "rebuild": "yarn run reinstall && yarn run build",
    "generate": "node ./generate-icons.js",
    "link": "yarn link @openworkshop/lib && yarn link @material-ui/core && yarn link @material-ui/lab && yarn link react && yarn link @types/react && yarn link prop-types && cd dist && yarn link",
    "dev": "tsc -b -w",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "build": "yarn lint && tsc -b --verbose --force && rollup -c"
  },
