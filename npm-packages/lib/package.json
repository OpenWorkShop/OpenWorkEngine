{
  "name": "@openworkshop/lib",
  "version": "0.0.0",
  "description": "API & utilities for OWS apps",
  "main": "./src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "pkg=$(cat ./dist/package.json) && rm -rf ./dist && mkdir ./dist && echo $pkg > ./dist/package.json",
    "uninstall": "rm -rf ./node_modules && rm -rf yarn.lock",
    "reinstall": "yarn run uninstall && yarn",
    "rebuild": "yarn run reinstall && yarn run build",
    "graphql": "graphql-codegen --config codegen.yml",
    "link": "yarn link react && yarn link @types/react && yarn link prop-types && cd dist && yarn link",
    "dev": "concurrently \"yarn graphql-codegen --watch\" \"tsc -b -w\"",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "build": "yarn lint && tsc -b --verbose --force && rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenWorkShop/OpenWorkShop.git"
  },
  "author": "OpenWorkShop, Inc.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OpenWorkShop/OpenWorkShop/issues"
  },
  "homepage": "https://github.com/OpenWorkShop/OpenWorkShop#readme",
  "dependencies": {},
