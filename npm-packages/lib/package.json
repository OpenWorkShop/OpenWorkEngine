    "js-logger": "^1.6.0",
    "react-i18next": "^11.7.3",
    "redux-logger": "^3.0.6",
    "redux-oidc": "^4.0.0-beta1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.2.5",
    "@graphql-codegen/cli": "1.19.1",
    "@graphql-codegen/introspection": "1.18.0",
